[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../game/target/debug/lib/game.so"
linux.release.x86_64 =   "res://../game/target/release/lib/game.so"
windows.debug.x86_64 =   "res://../game/target/debug//game.dll"
windows.release.x86_64 = "res://../game/target/release//game.dll"
macos.debug =            "res://../game/target/debug/lib/game.dylib"
macos.release =          "res://../game/target/release/lib/game.dylib"
macos.debug.arm64 =      "res://../game/target/debug/lib/game.dylib"
macos.release.arm64 =    "res://../game/target/release/lib/game.dylib"
